@model PX.Business.Models.EmailAccounts.EmailAccountManageModel

@{
    ViewBag.Header = "Email Account Management";
    ViewBag.Title = "Create";
    ViewBag.Description = "Create Email Account";
}
<div class="page-header">
    <h1>@ViewBag.Header
        <small>
            <i class="fa fa-angle-double-right"></i>
            @ViewBag.Title
        </small>
    </h1>
</div>

<div class="row">
    <div class="col-xs-10">
        @ShowStatusMessage()
        @using (Html.BeginForm("Create", "EmailAccounts", null, FormMethod.Post, new { @class = "form-horizontal" }))
        {
            @Html.EditorForModel(Model)
        }
    </div>
</div>